{
  "manifest_version": 2,
  "name": "BinaryConverter",
  "version": "1.0",

  "description": "Converts integers on screen to binary.",

  "icons": {
    "48": "icons/icon.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./findAndReplaceDOMText.js", "./binarify.js"]
    }
  ]
}

